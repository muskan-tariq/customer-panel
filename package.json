{
  "name": "tuffy-beauty",
  "version": "1.0.0",
  "description": "Tuffy Beauty E-commerce Full Stack",
  "scripts": {
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "start": "concurrently \"cd frontend && npm run preview\" \"cd backend && npm start\"",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "rm -rf node_modules && cd frontend && rm -rf node_modules && cd ../backend && rm -rf node_modules"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@google/generative-ai": "^0.21.0",
    "@stripe/stripe-js": "^5.2.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "concurrently": "^8.2.2",
    "framer-motion": "^11.13.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.468.0",
    "multer": "^1.4.5-lts.1",
    "stripe": "^17.4.0"
  },
  "homepage": "https://muskan-tariq.github.io/customer-panel"
}
